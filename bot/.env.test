# テスト用の環境変数設定（実際のトークンは使用しません）

# Discord Bot Token（テスト用ダミー）
DISCORD_BOT_TOKEN=test_token_here

# Bot コマンドプレフィックス
BOT_COMMAND_PREFIX=\!

# Bot 管理者ユーザーID
BOT_ADMIN_USER_IDS=123456789012345678

# 週次まとめ投稿先チャンネルID
DISCORD_SUMMARY_CHANNEL_ID=123456789012345678

# Firebase Service Account（テスト用ダミー）
FIREBASE_SERVICE_ACCOUNT={"type":"service_account","project_id":"test-project","private_key":"dummy"}

# Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=test-project

# Google Cloud リージョン
GOOGLE_CLOUD_LOCATION=asia-northeast1

# Google Drive フォルダID
GOOGLE_DRIVE_FOLDER_ID=test_folder_id

# スケジューラー設定
AUTO_START_SCHEDULER=false
WEEKLY_SCHEDULE_DAY=monday
WEEKLY_SCHEDULE_TIME=09:00

# システム設定
CLEANUP_LOCAL_FILES=true
EOF < /dev/null